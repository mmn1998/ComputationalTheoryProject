        <Button x:Name="back" Content="Back to main menu" HorizontalAlignment="Left" VerticalAlignment="Top" Width="116" Margin="227,250,0,0" Background="White" Height="28" Click="button_Click"/>
        <Button x:Name="button" Content="Compute number" HorizontalAlignment="Left" VerticalAlignment="Top" Width="116" Margin="227,217,0,0" Height="28" Background="White" Click="button_Click_1"/>
